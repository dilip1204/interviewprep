package com.learn.thread;

public class SynchornizeTest {
	
	public static int counter =0;
	
	public void process() {
		Thread t1 = new Thread(new Runnable() {
			
			@Override
			public void run() {
				for(int i=0;i<100;i++) {
					++counter;
				}
			}
		});
		
		
		Thread t2 = new Thread(new Runnable() {
			
			@Override
			public void run() {
				for(int i=0;i<100;i++) {
					++counter;
				}
			}
		});
		
		t1.start();
		t2.start();
		
		t1.join();
		
	}

}
